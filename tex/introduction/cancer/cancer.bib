	pages = {27--40},
	file = {Full Text PDF:/Users/giltomas/Library/Application Support/Firefox/Profiles/oi17mj7r.default/zotero/storage/WWFI7PHX/Stephens et al. - 2011 - Massive Genomic Rearrangement Acquired in a Single.pdf:application/pdf;Snapshot:/Users/giltomas/Library/Application Support/Firefox/Profiles/oi17mj7r.default/zotero/storage/2MV3ES4H/S0092-8674(10)01377-2.html:text/html}
