	month = mar,
	year = {2009},
	pmid = {19261858},
	keywords = {growth, invasion, progression},
	pages = {4828--4833},
	file = {Full Text PDF:/Users/giltomas/Library/Application Support/Firefox/Profiles/oi17mj7r.default/zotero/storage/IRNQXJUF/Siegmund et al. - 2009 - Inferring clonal expansion and cancer stem cell dy.pdf:application/pdf;Snapshot:/Users/giltomas/Library/Application Support/Firefox/Profiles/oi17mj7r.default/zotero/storage/FRPZZH75/4828.html:text/html}
}

