	number = {38},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {Miller, Lance D. and Smeds, Johanna and George, Joshy and Vega, Vinsensius B. and Vergara, Liza and Ploner, Alexander and Pawitan, Yudi and Hall, Per and Klaar, Sigrid and Liu, Edison T. and Bergh, Jonas},
