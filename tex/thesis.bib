	month = oct,
	year = {2003},
	pmid = {12777628},
	pages = {6958--6963},
	file = {Full Text PDF:/Users/giltomas/Library/Application Support/Zotero/Profiles/eh5dlmjl.default/zotero/storage/MFHPUFNB/Vasselli et al. - 2003 - Predicting survival in patients with metastatic ki.pdf:application/pdf;Snapshot:/Users/giltomas/Library/Application Support/Zotero/Profiles/eh5dlmjl.default/zotero/storage/A8KTA6VV/6958.html:text/html}
