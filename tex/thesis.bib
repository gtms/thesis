	pmid = {22271477},
	pages = {647--660},
	file = {Full Text PDF:/Users/giltomas/Library/Application Support/Zotero/Profiles/eh5dlmjl.default/zotero/storage/BD3X5SGG/Tran et al. - 2012 - Cancer Genomics Technology, Discovery, and Transl.pdf:application/pdf;Snapshot:/Users/giltomas/Library/Application Support/Zotero/Profiles/eh5dlmjl.default/zotero/storage/KXJJB2ZN/647.html:text/html}
