	pages = {7388--7393},
	file = {Full Text PDF:/Users/giltomas/Library/Application Support/Zotero/Profiles/eh5dlmjl.default/zotero/storage/773WMQKG/Su et al. - 2001 - Molecular Classification of Human Carcinomas by Us.pdf:application/pdf;Snapshot:/Users/giltomas/Library/Application Support/Zotero/Profiles/eh5dlmjl.default/zotero/storage/VMAI88V7/7388.full.html:text/html}
