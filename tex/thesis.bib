	keywords = {Breast Neoplasms, Female, Gene Expression Profiling, Gene Expression Regulation, Neoplastic, Humans, Mutation, Oligonucleotide Array Sequence Analysis, Predictive Value of Tests, prognosis, Transcription, Genetic, Tumor Suppressor Protein p53},
	pages = {13550--13555}
}

