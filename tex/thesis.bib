	pmid = {19174838},
	keywords = {Animals, Databases, Genetic, Data Interpretation, Statistical, Gene Expression Profiling, Genome-Wide Association Study, Humans, Oligonucleotide Array Sequence Analysis, Peer Review, Research, Publications, Reproducibility of Results},
	pages = {149--155}
}

