	pmid = {16899776},
	keywords = {Analysis of Variance, Breast Neoplasms, Female, Gene Expression, Gene Expression Profiling, Humans, Models, Genetic, Phenotype, prognosis, Proportional Hazards Models, Receptor, erbB-2, Receptors, Estrogen, Survival Analysis},
	pages = {560--569}
}

