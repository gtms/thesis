	pmid = {10963602},
	keywords = {Breast Neoplasms, DNA, Neoplasm, Female, Gene Expression, Gene Expression Profiling, Genes, erbB-2, Humans, Oligonucleotide Array Sequence Analysis, Phenotype, Tumor Cells, Cultured},
	pages = {747--752}
