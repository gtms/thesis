	pmid = {12469122},
	keywords = {Adenocarcinoma, Gene Expression Profiling, Gene Expression Regulation, Neoplastic, Humans, Neoplasm Metastasis, Neoplasms, Organ Specificity, Predictive Value of Tests, prognosis, Survival Analysis},
