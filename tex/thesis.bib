	keywords = {Escherichia coli, Gene Expression Profiling, Gene Expression Regulation, Bacterial, Genome, Bacterial, Oligonucleotide Array Sequence Analysis, Open Reading Frames, RNA, Antisense, RNA, Bacterial, RNA, Messenger, Transcription, Genetic},
	pages = {1262--1268}
